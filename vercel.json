{
  "installCommand": "npm install --legacy-peer-deps && node scripts/fix-fork-ts-checker.js",
  "build": {
    "env": {
      "SKIP_PREFLIGHT_CHECK": "true",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}